<header>
    <div id="PreHeader">
        {{> submenu}}
    </div>
    <div id="MainHeader">
        <div id="LogoHeader">Lista del Sol</div>
        <div id="MenuHeader">{{>menu}}</div>
    </div>
</header>
<main>
    <div id="container">
        <h1>Perfíl de Usuario</h1>
        <div id="perfil">
            <h2>Datos de Usuario</h2>
        
            <div class="espaciado">
                <p><strong>Nombre:</strong> {{usuario.first_name}} {{usuario.last_name}}</p>
                <p><strong>Edad:</strong> {{usuario.age}}</p>
                <p><strong>Correo Electrónico:</strong> {{usuario.email}}</p>
            </div>
        </div>
    </div>
</main>
<footer>
    <ul>
        <li><a href="/home" >Página de Inicio</a></li>
    </ul>
    
</footer>